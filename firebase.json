{
  "hosting": {
    "public": "dist/charuabusfrontend/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "predeploy": [
    "npm run build"
  ]
}
